
@model List<Chaine>

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
</head>
<body>
<h1>Liste des chaines</h1>
<table class="table table-hover">
    <th>Chaine</th>
    <th>Prix</th>
    <th>Signal</th>
        @foreach (var channel in Model)
        {
          <tr>
            <td>@channel.nomChaine</td>
            <td>@channel.prix Ar</td>
            <td>@channel.signal</td>
          </tr>
        }
</table>
</body>
</html>